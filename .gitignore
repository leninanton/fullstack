#include <iostream>

int main() {
    // Инициализация массива числами
    int numbers[] = { 1, 2, 3, 4, 5 };

    // Вывод элементов массива на экран
    std::cout << "Массив чисел:" << std::endl;
    for (int i = 0; i < sizeof(numbers) / sizeof(numbers[0]); i++) {
        std::cout << numbers[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}
